rm -f core/main.o core/cmdlist.o io/disp.o io/interrupt.o io/kbd.o io/hd.o mm/memory.o mm/page.o fs/block.o fs/fs.o fs/ext4.o fs/ext2.o core/corestart.o core/entrance.o core.elf core.map core.bin boot.o boot.bin *~
gcc -I/home/david/C_C++/Porcelix/myLoader -Iinclude   -c -o boot.o boot.S
objcopy -O binary boot.o boot.bin
gcc -Wall -o2 -I/home/david/C_C++/Porcelix/myLoader -Iinclude -fno-stack-protector -fno-builtin   -c -o core/main.o core/main.c
gcc -Wall -o2 -I/home/david/C_C++/Porcelix/myLoader -Iinclude -fno-stack-protector -fno-builtin   -c -o core/cmdlist.o core/cmdlist.c
gcc -Wall -o2 -I/home/david/C_C++/Porcelix/myLoader -Iinclude -fno-stack-protector -fno-builtin   -c -o io/disp.o io/disp.c
gcc -Wall -o2 -I/home/david/C_C++/Porcelix/myLoader -Iinclude -fno-stack-protector -fno-builtin   -c -o io/interrupt.o io/interrupt.c
gcc -Wall -o2 -I/home/david/C_C++/Porcelix/myLoader -Iinclude -fno-stack-protector -fno-builtin   -c -o io/kbd.o io/kbd.c
gcc -Wall -o2 -I/home/david/C_C++/Porcelix/myLoader -Iinclude -fno-stack-protector -fno-builtin   -c -o io/hd.o io/hd.c
gcc -Wall -o2 -I/home/david/C_C++/Porcelix/myLoader -Iinclude -fno-stack-protector -fno-builtin   -c -o mm/memory.o mm/memory.c
gcc -Wall -o2 -I/home/david/C_C++/Porcelix/myLoader -Iinclude -fno-stack-protector -fno-builtin   -c -o mm/page.o mm/page.c
gcc -Wall -o2 -I/home/david/C_C++/Porcelix/myLoader -Iinclude -fno-stack-protector -fno-builtin   -c -o fs/block.o fs/block.c
gcc -Wall -o2 -I/home/david/C_C++/Porcelix/myLoader -Iinclude -fno-stack-protector -fno-builtin   -c -o fs/fs.o fs/fs.c
gcc -Wall -o2 -I/home/david/C_C++/Porcelix/myLoader -Iinclude -fno-stack-protector -fno-builtin   -c -o fs/ext4.o fs/ext4.c
gcc -Wall -o2 -I/home/david/C_C++/Porcelix/myLoader -Iinclude -fno-stack-protector -fno-builtin   -c -o fs/ext2.o fs/ext2.c
gcc -I/home/david/C_C++/Porcelix/myLoader -Iinclude   -c -o core/corestart.o core/corestart.S
gcc -I/home/david/C_C++/Porcelix/myLoader -Iinclude   -c -o core/entrance.o core/entrance.S
ld core/main.o core/cmdlist.o io/disp.o io/interrupt.o io/kbd.o io/hd.o mm/memory.o mm/page.o fs/block.o fs/fs.o fs/ext4.o fs/ext2.o core/corestart.o core/entrance.o -T myLoader.lds -o core.elf -M > core.map
objcopy -O binary core.elf core.bin
